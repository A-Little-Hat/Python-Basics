      },
      "source": [
        "def rev(num):\n",
        "  revNum = 0\n",
        "  while num != 0:\n",
        "    digit = num % 10\n",
        "    revNum = revNum * 10 + digit\n",
        "    num //= 10\n",
        "  return revNum"
      ],
      "execution_count": 13,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "mHfYWjhLmOSn"
      },
      "source": [
        "reversedNumber = rev(num)"
      ],
      "execution_count": 14,
      "outputs": []
