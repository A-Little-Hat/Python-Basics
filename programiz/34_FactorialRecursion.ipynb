        "n=int(input(\"enter a number \"))\n",
        "def fact(n):\n",
        "  if(n>=2):\n",
        "    return n*fact(n-1)\n",
        "  if(n<0):\n",
        "    return print(\"wrong input\")\n",
        "  else:\n",
        "    return 1\n",
        "print(fact(n))"
