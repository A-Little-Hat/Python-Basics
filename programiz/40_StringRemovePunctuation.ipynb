      "source": [
        "def Punctuation(string):\n",
        "\tpunctuations = '''!()-[]{};:'\"\\,<>./?@#$%^&*_~'''\n",
        "\tfor x in string.lower():\n",
        "\t\tif x in punctuations:\n",
        "\t\t\tstring = string.replace(x, \"\")\n",
        "\tprint(string)"
      ],
      "execution_count": 22,
      "outputs": []
