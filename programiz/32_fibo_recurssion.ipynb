        "def fibo(n):\n",
        "  if(n<=1):\n",
        "    return n\n",
        "  else:\n",
        "    return (fibo(n-1)+fibo(n-2))\n",
        "n=int(input(\"enter terms\"))\n",
        "if n <= 0:\n",
        "   print(\"Plese enter a positive integer\")\n",
        "else:\n",
        "   print(\"Fibonacci sequence:\")\n",
        "   for i in range(n):\n",
        "       print(fibo(i))"
      ],
